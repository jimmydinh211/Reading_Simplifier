{
  "manifest_version": 3,
  "name": "Readable",
  "description": "Base Level Extension",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo512.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}